{
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "env": {
        "TAMAGUI_TARGET": "native",
        "NEXT_PUBLIC_SHOW_BABYLON_INSPECTOR": "false"
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "TAMAGUI_TARGET": "native",
        "NEXT_PUBLIC_SHOW_BABYLON_INSPECTOR": "false"
      }
    }
  }
}
